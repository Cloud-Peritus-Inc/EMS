<?xml version="1.0" encoding="UTF-8"?>
<ApprovalProcess xmlns="http://soap.sforce.com/2006/04/metadata">
    <active>true</active>
    <allowRecall>false</allowRecall>
    <allowedSubmitters>
        <type>owner</type>
    </allowedSubmitters>
    <approvalPageFields>
        <field>EMS_TM_Project__c</field>
        <field>EMS_TM_ProjectTask__c</field>
        <field>Status__c</field>
        <field>Name</field>
        <field>EMS_TM_Mon__c</field>
        <field>EMS_TM_Tue__c</field>
        <field>EMS_TM_Wed__c</field>
        <field>EMS_TM_Thu__c</field>
        <field>EMS_TM_Fri__c</field>
        <field>EMS_TM_Sat__c</field>
        <field>EMS_TM_Sun__c</field>
        <field>EMS_TM_OtherTask__c</field>
        <field>EMS_TM_Remarks__c</field>
        <field>Owner</field>
    </approvalPageFields>
    <approvalStep>
        <allowDelegate>false</allowDelegate>
        <assignedApprover>
            <approver>
                <type>adhoc</type>
            </approver>
        </assignedApprover>
        <label>Status Approve</label>
        <name>Status_Approve</name>
    </approvalStep>
    <enableMobileDeviceAccess>false</enableMobileDeviceAccess>
    <entryCriteria>
        <criteriaItems>
            <field>EMS_TM_Timesheet_Record__c.Status__c</field>
            <operation>notEqual</operation>
            <value>Saved,Approved,Rejected</value>
        </criteriaItems>
    </entryCriteria>
    <finalApprovalActions>
        <action>
            <name>Approved_Upadte</name>
            <type>FieldUpdate</type>
        </action>
    </finalApprovalActions>
    <finalApprovalRecordLock>true</finalApprovalRecordLock>
    <finalRejectionActions>
        <action>
            <name>Rejected</name>
            <type>FieldUpdate</type>
        </action>
    </finalRejectionActions>
    <finalRejectionRecordLock>false</finalRejectionRecordLock>
    <initialSubmissionActions>
        <action>
            <name>Submitted</name>
            <type>FieldUpdate</type>
        </action>
    </initialSubmissionActions>
    <label>Timesheet Records Approval</label>
    <processOrder>1</processOrder>
    <recordEditability>AdminOrCurrentApprover</recordEditability>
    <showApprovalHistory>true</showApprovalHistory>
</ApprovalProcess>
