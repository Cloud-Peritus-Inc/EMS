    public static HttpResponse uploadFile( ContentVersion contentVersion, String parentId  ) {
        
        String boundary = '----------9889464542212';
        String delimiter = '\r\n--' + boundary + '\r\n';
        String close_delim = '\r\n--' + boundary + '--';
        
        String bodyEncoded = EncodingUtil.base64Encode(contentVersion.VersionData);
        
        String body = delimiter + 'Content-Type: application/json\r\n\r\n' + '{ "title" : "' + contentVersion.Title + '",' + ' "mimeType" : "'+mapMimeTypes(contentVersion.Filetype)+'", "parents": [{ "id": "'+parentId+'" }] }';
        body += delimiter + 'Content-Type: ' + mapMimeTypes(contentVersion.Filetype) + '\r\n' + 'Content-Transfer-Encoding: base64\r\n' + '\r\n' + bodyEncoded + close_delim;
        
        HttpRequest req = new HttpRequest();
        req.setEndpoint('callout:New_Drive/upload/drive/v2/files?uploadType=multipart');
        req.setHeader('Content-Type', 'multipart/mixed; boundary="' + boundary + '"');
        req.setHeader('Content-length', String.valueOf(body.length()));
        req.setBody(body);
        req.setMethod('POST');
        req.setTimeout(60 * 1000);
        
        System.debug('****EndPoint***'+ req.getEndpoint()  );
        Http http = new Http();
        
        HttpResponse res = new HttpResponse();
        
        if(!test.isRunningTest()){
            res = http.send(req);
        }else{
            res.setHeader('content-type', 'application/json');
            res.setBody('{ "kind": "drive#permission", "id": "15503934445050923408", "type": "user", "role": "reader" }');
            res.setStatusCode(200);
            return res;
        }
        
        
        System.debug('****response***'+ res  );
        System.debug('****response***'+ res.getBody()  );
        
        return res;
    }
