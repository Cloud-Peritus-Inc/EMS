public class CreateDriveMainFolder {
    public static void createOnboardRequestFolderinDrive(string ApplicantID){
        HttpRequest req = new HttpRequest();
        req.setMethod('POST');
        req.setEndpoint('callout:New_Drive/drive/v3/files');
        req.setHeader('content-type', 'application/json');
        String body = '{"name" : "'+ ApplicantID +'","mimeType" : "application/vnd.google-apps.folder"}';
        req.setTimeout(60*1000);
        req.setBody(body);
        Http http = new Http();
        HttpResponse res = http.send(req);
        System.debug('===== Response==='+ res.getBody());
        Jsonwrapper  driveResponse = (Jsonwrapper)JSON.deserialize(res.getBody(),Jsonwrapper.class);
        
        EMS_EM_Onboarding_Request__c RequestList = [select id,EMS_EM_Name__c,Is_Inegration_Needed__c, Google_Drive_ID__c from EMS_EM_Onboarding_Request__c where EMS_EM_Name__c =:ApplicantID LIMIT 1];
        RequestList.Google_Drive_ID__c = driveResponse.id;
        RequestList.Is_Inegration_Needed__c = RequestList.Google_Drive_ID__c!= null ? true : false;
        try{
            Update RequestList;
            System.debug('RequestList====> '+RequestList);
        } catch(Exception ex){
            System.debug('***Exception  '+ ex.getMessage());
        }
        System.debug('driveResponse'+driveResponse.id);
    }
