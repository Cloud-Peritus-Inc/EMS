<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Level_Approval_Must_Be_Less_Than_3</fullName>
    <active>true</active>
    <errorConditionFormula>Override_the_standard_level_of_approval__c &amp;&amp; Level_of_Approval__c &gt; 3</errorConditionFormula>
    <errorMessage>The Level of Approval value must be less than or equal to 3</errorMessage>
</ValidationRule>
