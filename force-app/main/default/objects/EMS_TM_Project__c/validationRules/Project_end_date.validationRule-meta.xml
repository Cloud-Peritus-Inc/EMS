<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Project_end_date</fullName>
    <active>true</active>
    <errorConditionFormula>AND(ISNEW(), EMS_TM_EndDate__c &lt; EMS_TM_StartDate__c)</errorConditionFormula>
    <errorMessage>End date should be grater than or equal start date</errorMessage>
</ValidationRule>
