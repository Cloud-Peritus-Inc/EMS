<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Day7</fullName>
    <active>false</active>
    <errorConditionFormula>OR(
    EMS_TM_Sat__c  &gt; 24.00,
    EMS_TM_Sat__c  &lt; 0.00,
    ISBLANK( EMS_TM_Sat__c ) 
)</errorConditionFormula>
    <errorDisplayField>EMS_TM_Sat__c</errorDisplayField>
    <errorMessage>Working hours should be in between 1 to 10 and not Blank</errorMessage>
</ValidationRule>
