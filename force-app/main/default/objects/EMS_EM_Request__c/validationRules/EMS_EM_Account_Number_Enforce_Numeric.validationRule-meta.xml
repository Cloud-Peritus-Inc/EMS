<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>EMS_EM_Account_Number_Enforce_Numeric</fullName>
    <active>true</active>
    <errorConditionFormula>NOT(REGEX(Account_Number__c,&quot;^[0-9]*$&quot;))</errorConditionFormula>
    <errorMessage>Invalid Account Number</errorMessage>
</ValidationRule>
