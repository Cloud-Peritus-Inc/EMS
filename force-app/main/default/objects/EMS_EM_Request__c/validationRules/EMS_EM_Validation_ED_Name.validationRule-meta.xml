<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>EMS_EM_Validation_ED_Name</fullName>
    <active>true</active>
    <description>Allow only letters for Emergency details Name field</description>
    <errorConditionFormula>NOT(REGEX( EMS_EM_ED_Name__c ,&quot;^[a-z A-Z]*$&quot;))</errorConditionFormula>
    <errorDisplayField>EMS_EM_ED_Name__c</errorDisplayField>
    <errorMessage>Invalid Emergency details Name</errorMessage>
</ValidationRule>
