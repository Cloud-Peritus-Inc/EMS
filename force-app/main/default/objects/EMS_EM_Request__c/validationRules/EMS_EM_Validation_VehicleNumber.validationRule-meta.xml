<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>EMS_EM_Validation_VehicleNumber</fullName>
    <active>true</active>
    <errorConditionFormula>IF( ISPICKVAL(EMS_EM_Request_Type__c, &quot;Vehicle Details&quot;),
NOT(REGEX(Vehicle_Number__c, &quot;[a-zA-Z0-9]*&quot;)),false)</errorConditionFormula>
    <errorDisplayField>Vehicle_Number__c</errorDisplayField>
    <errorMessage>Invalid Vehicle Number</errorMessage>
</ValidationRule>
