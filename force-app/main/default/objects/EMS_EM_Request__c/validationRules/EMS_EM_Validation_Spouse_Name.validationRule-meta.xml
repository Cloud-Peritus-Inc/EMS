<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>EMS_EM_Validation_Spouse_Name</fullName>
    <active>true</active>
    <description>VR for allow only letters in Spouse name</description>
    <errorConditionFormula>NOT(REGEX( EMS_EM_Spouse_Name__c ,&quot;^[a-z A-Z]*$&quot;))</errorConditionFormula>
    <errorDisplayField>EMS_EM_Spouse_Name__c</errorDisplayField>
    <errorMessage>Invalid Name format for Spouse Name</errorMessage>
</ValidationRule>
